{
    "appId": "com.leonard-penzenstadler.reseach",
    "files": [
      "dist-electron/**",
      "dist-react/**",
      "package.json"
    ],
    "extraResources": [
      "dist-electron/preload.cjs",
      "src/assets/**"
    ],
    "icon": "./desktopIcon.png",
    "mac": {
      "target": "dmg",
      "icon": "./desktopIcon.icns"
    },
    "linux": {
      "target": "AppImage",
      "category": "Utility",
      "icon": "./desktopIcon.png"
    },
    "win": {
      "target": ["portable", "msi"],
      "icon": "./desktopIcon.ico"
    },
    "npmRebuild": false,
    "buildDependenciesFromSource": false
}


